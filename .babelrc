{
  "presets": [
    [
      "env",
      {
        "browsers": [
          "last 2 versions",
          "ie >= 10"
        ],
        "modules": false
      }
    ],
    "react",
    "babel-preset-stage-0"
  ],
  "plugins": [
    [
      "transform-runtime",
      {
        "polyfill": false,
        "regenerator": true
      }
    ],
    "react-hot-loader/babel"
  ],
  "env": {
    "test": {
      "presets": [
        [
          "env",
          {
            "modules": "commonjs"
          }
        ]
      ]
    }
  }
}
